  "test_scenarios": {
    "high_volume": {
      "description": "High volume trap scenario for load testing",
      "trap_count": 1000,
      "interval_ms": 10,
      "trap_types": ["coldStart", "warmStart", "linkDown", "linkUp"]
    },
    "burst": {
      "description": "Burst scenario with rapid trap generation",
      "trap_count": 100,
      "interval_ms": 1,
      "trap_types": ["authenticationFailure"]
    },
    "mixed_vendors": {
      "description": "Mixed vendor trap scenario",
      "trap_count": 50,
      "interval_ms": 100,
      "trap_types": [
        "cisco.configChange",
        "juniper.alarmSet",
        "hp.serverAlert",
        "cyberpower.upsOnBattery"
      ]
    },
    "correlation_test": {
      "description": "Scenario for testing event correlation",
      "sequences": [
        {
          "description": "Interface flapping",
          "traps": [
            {"type": "linkDown", "delay_ms": 0},
            {"type": "linkUp", "delay_ms": 1000},
            {"type": "linkDown", "delay_ms": 2000},
            {"type": "linkUp", "delay_ms": 3000}
          ]
        },
        {
          "description": "UPS power cycle",
          "traps": [
            {"type": "cyberpower.upsOnBattery", "delay_ms": 0},
            {"type": "cyberpower.upsPowerRestored", "delay_ms": 5000}
          ]
        }
      ]
    }
  },
  "malformed_packets": {
    "invalid_version": {
      "description": "Packet with invalid SNMP version",
      "modifications": {
        "version": 99
      }
    },
    "empty_community": {
      "description": "Packet with empty community string",
      "modifications": {
        "community": ""
      }
    },
    "invalid_pdu_type": {
      "description": "Packet with invalid PDU type",
      "modifications": {
        "pdu_type": 99
      }
    },
    "no_varbinds": {
      "description": "Packet with no varbinds",
      "modifications": {
        "varbinds": []
      }
    },
    "invalid_oid": {
      "description": "Packet with malformed OID",
      "modifications": {
        "varbinds": [
          {
            "oid": "invalid.oid.format",
            "type": "string",
            "value": "test"
          }
        ]
      }
    },
    "oversized_packet": {
      "description": "Packet exceeding maximum size",
      "modifications": {
        "oversized_value": true,
        "size_bytes": 100000
      }
    }
  }
